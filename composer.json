{
    "name": "acquia/acquia-ra-composer",
    "description": "Project json for Acquia Remote Administration Composer builds",
    "type": "project",
    "license": "MIT",
    "require": {
        "drupal/core": "^8.3",
        "drupal/admin_toolbar": "^1.0",
        "drupal/dropzonejs": "^1.0",
        "drupal/entity_embed": "dev-1.x",
        "drupal/entity_browser": "dev-1.x",
        "drupal/file_entity": "^2.0",
        "drupal/footnotes": "2.0",
        "drupal/geolocation": "1.x-dev",
        "drupal/google_analytics": "^2.0",
        "drupal/adminimal_theme": "^1.0",
        "enyo/dropzone": "4.2.0",
        "desandro/masonry": "^4.1",
        "desandro/imagesloaded": "^4.1",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "require-dev": {
        "drupal/devel": "^1.0",
        "drupal/stage_file_proxy": "^1.0"
    },
    "conflict": {
        "drupal/core": "7.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "installer-types": [
            "library",
            "component"
        ],
        "installer-paths": {
            "docroot/core": [type:drupal-core],
            "docroot/modules/contrib/{$name}": [type:drupal-module],
            "docroot/modules/custom/{$name}": [type:drupal-custom-module],
            "docroot/profiles/contrib/{$name}": [type:drupal-profile],
            "docroot/profiles/custom/{$name}": [type:drupal-custom-profile],
            "docroot/themes/contrib/{$name}": [type:drupal-theme],
            "docroot/themes/custom/{$name}": [type:drupal-custom-theme],
            "docroot/libraries/{$name}": [type:drupal-library],
            "docroot/libraries/masonry": [desandro/masonry],
            "docroot/libraries/imagesloaded": [desandro/imagesloaded],
            "docroot/libraries/dropzone": [enyo/dropzone]
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/development.services.yml",
                "docroot/sites/development.services.yml"
            ],
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        },
        "patches": [],
        "enable-patching": true
    }
}
